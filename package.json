  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node prisma/seed.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/shared/database/prisma/seed.ts && ts-node -r tsconfig-paths/register src/scripts/setup-clinics.ts",
    "prisma:generate": "ts-node src/shared/database/prisma/generate-all-schemas.ts",
    "prisma:generate:main": "prisma generate --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:generate:tenant": "prisma generate --schema=./src/shared/database/prisma/tenant.schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:seed": "ts-node src/shared/database/prisma/seed.ts",
    "prisma:studio": "prisma studio --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./src/shared/database/prisma/schema.prisma",
    "setup:clinics": "ts-node -r tsconfig-paths/register src/scripts/setup-clinics.ts",
    "docker:start": "docker-compose up -d --build",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v",
    "docker:rebuild": "docker-compose down && docker-compose up -d --build",
    "tenant:update-schema": "ts-node src/shared/database/prisma/tenant-schema-update.ts"
  },
