    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.2.5",
    "prisma": "^6.8.2",
    "prisma-erd-generator": "^2.0.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
