{
  "name": "healthcare-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:dev:docker": "cross-env NODE_ENV=development docker-compose -f docker-compose.dev.yml up -d",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:prod:docker": "cross-env NODE_ENV=production docker-compose -f docker-compose.yml up -d",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./src/shared/database/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/shared/database/prisma/schema.prisma --force",
    "seed:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/shared/database/prisma/seed.ts",
    "seed:dev:docker": "cross-env NODE_ENV=development docker-compose -f docker-compose.dev.yml exec api npm run seed:dev",
    "seed:prod": "cross-env NODE_ENV=production node dist/shared/database/prisma/seed.js",
    "seed:prod:docker": "cross-env NODE_ENV=production docker-compose -f docker-compose.yml exec api npm run seed:prod",
    "deploy:dev": "npm run prisma:generate && npm run prisma:migrate && npm run seed:dev",
    "deploy:prod": "npm run build && npm run prisma:generate && npm run prisma:migrate && npm run seed:prod",
    "docker:start": "cross-env NODE_ENV=development docker-compose -f docker-compose.dev.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v",
    "docker:rebuild": "docker-compose -f docker-compose.dev.yml down && cross-env NODE_ENV=development docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:prod": "cross-env NODE_ENV=production docker-compose -f docker-compose.yml up -d --build",
    "docker:prod:logs": "docker-compose -f docker-compose.yml logs -f",
    "docker:prod:stop": "docker-compose -f docker-compose.yml down",
    "env:check": "node -e \"console.log('Current NODE_ENV:', process.env.NODE_ENV)\"",
    "validate:env": "ts-node scripts/validate-env.ts"
  },
